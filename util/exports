export GCC_HOST_VERSION=9
export LLVM_HOST_VERSION=10
export CC=/usr/bin/clang-$LLVM_HOST_VERSION
export CXX=/usr/bin/clang++-$LLVM_HOST_VERSION
export LLVM_CMAKE_FLAGS="-D LLVM_DIR='/usr/lib/llvm-$LLVM_HOST_VERSION/cmake'"
export LLVM_CMAKE_FLAGS="$LLVM_CMAKE_FLAGS -D ENABLE_LLVM=ON"
export CFLAGS=-Werror
export CXXFLAGS=-Werror
